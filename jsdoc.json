{
  "plugins": [
    "./node_modules/brighterscript-jsdocs-plugin/convert-brighterscript-docs.js",
    "plugins/markdown"
  ],
  "source": {
    "include": [
      "source",
      "components"
    ],
    "includePattern": ".+\\.br?s$",
    "excludePattern": "(roku_modules)"
  },
  "extendedConfig": {},
  "opts": {
    "encoding": "utf8",
    "recurse": true,
    "template": "node_modules/clean-jsdoc-theme",
    "brighterscript-jsdocs-plugin": {
      "addModule": true,
      "escapeHTMLCharacters": false
    },
    "theme_opts": {
      "default_theme": "fallback-dark",
      "base_url": "https://cewert.github.io/jellyrock-code-docs/",
      "title": "JellyRock Code Documentation",
      "homepageTitle": "JellyRock Code Documentation",
      "displayModuleHeader": true,
      "menu": [
        {
          "title": "Source Code",
          "link": "https://github.com/cewert/jellyrock",
          "target": "_blank",
          "class": "external-link",
          "id": "github-link"
        },
        {
          "title": "Jellyfin Server",
          "link": "https://jellyfin.org/",
          "target": "_blank",
          "class": "external-link",
          "id": "jellyfin-link"
        }
      ],
      "footer": "<span class=\"jsdoc-message\">Automatically generated using <a href=\"https://github.com/jsdoc/jsdoc\" target=\"_blank\">JSDoc</a> and the <a href=\"https://github.com/ankitskvmdam/clean-jsdoc-theme\" target=\"_blank\">clean-jsdoc-theme</a>.</span>"
    }
  },
  "markdown": {
    "hardwrap": false,
    "idInHeadings": true
  }
}