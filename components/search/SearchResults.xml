<?xml version="1.0" encoding="utf-8"?>
<component name="searchResults" extends="JRGroup">
  <children>
    <LayoutGroup layoutDirection="horiz" id="SearchAlphabox" translation="[70, 120]">
      <SearchBox id="SearchBox" visible="true" focusable="true" />
    </LayoutGroup>

    <SearchRow id="searchSelect" focusable="true" />

    <LabelPrimaryLarge id="SearchHelpText" width="1394" horizAlign="center" text="You can search for Titles, People, Live TV Channels and more" translation="[470, 440]" />

    <OptionsSlider id="options" />
  </children>
  <interface>
    <field id="query" type="string" alwaysNotify="true" />
    <field id="selectedItem" type="node" alwaysNotify="true" />
    <field id="quickPlayNode" type="node" alwaysNotify="true" />
    <field id="imageDisplayMode" type="string" value="scaleToZoom" />
    <field id="searchAlpha" type="string" alias="SearchBox.search_values" alwaysNotify="true" onChange="searchMedias" />
  </interface>
</component>