<?xml version="1.0" encoding="utf-8"?>
<component name="SongItem" extends="Group">
  <children>
    <LayoutGroup layoutDirection="horiz" horizAlignment="left" itemSpacings="[5, 0]" translation="[0, 15]">
      <LabelSmall id="trackNumber" horizAlign="left" vertAlign="center" width="80" bold="true" />
      <LabelSmall id="itemText" horizAlign="left" vertAlign="center" width="930" bold="true" />
      <LabelSmall id="tracklength" horizAlign="right" vertAlign="center" width="150" bold="true" />
    </LayoutGroup>
  </children>
  <interface>
    <field id="itemContent" type="node" onChange="itemContentChanged" />
    <field id="itemHasFocus" type="boolean" onChange="focusChanged" />
  </interface>
</component>