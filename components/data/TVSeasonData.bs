import "pkg:/source/utils/misc.bs"
sub setFields()
  datum = m.top.json

  m.top.id = datum.id
  m.top.title = datum.name
  m.top.overview = datum.overview

  setPoster()
end sub

sub setPoster()
  if isValid(m.top.image)
    m.top.posterURL = m.top.image.url
  else
    m.top.posterURL = ""
  end if

end sub
