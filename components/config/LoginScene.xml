<?xml version="1.0" encoding="utf-8"?>
<component name="LoginScene" extends="JRScreen">
  <children>
    <LabelPrimaryLarge text="Enter Configuration" id="prompt" translation="[150, 150]" bold="true" />
    <ConfigList id="configOptions" translation="[150, 240]" />
    <CheckList id="onOff" translation="[200, 450]" />
    <JRButtonGroup id="buttons" itemSpacings="72" translation="[150,550]">
      <TextButton id="submit" minWidth="330" text="Submit" />
      <TextButton id="quickConnect" minWidth="330" text="Quick Connect" />
    </JRButtonGroup>
    <LabelErrorMedium text="" id="alert" wrap="true" width="1620" translation="[150, 680]" />

    <Timer id="quickConnectTimer" duration="3" repeat="true" />
  </children>
</component>