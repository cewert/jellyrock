<?xml version="1.0" encoding="utf-8"?>
<component name="ListPoster" extends="Group">
  <children>
    <RectangleBackgroundSecondary id="backdrop" />

    <ScrollingLabelPrimarySmaller id="Series" bold="true" horizAlign="center" repeatCount="0" visible="false" />

    <JFPoster id="poster" loadDisplayMode="scaleToFit" />

    <ScrollingLabelPrimarySmaller id="title" bold="true" horizAlign="center" repeatCount="0" visible="false" />
    <LabelPrimarySmaller id="staticTitle" bold="true" horizAlign="center" wrap="false" />
  </children>
  <interface>
    <field id="itemContent" type="node" onChange="itemContentChanged" />
    <field id="itemWidth" type="integer" />
    <field id="itemHasFocus" type="boolean" onChange="focusChanged" />
  </interface>
</component>