<?xml version="1.0" encoding="utf-8"?>
<component name="TextButton" extends="Group" focusable="true">
  <children>
    <Poster id="buttonFocusBorder" uri="pkg:/images/white.9.png" visible="false" />
    <Poster id="buttonBackground" uri="pkg:/images/white.9.png" />
    <LabelPrimaryMedium id="buttonText" horizAlign="center" vertAlign="center" />
  </children>
  <interface>
    <field id="background" type="color" value="" />
    <field id="focusBackground" type="color" value="" />
    <field id="textColor" type="color" value="" />
    <field id="textFocusColor" type="color" value="" />
    <field id="text" type="string" value="" />
    <field id="padding" type="integer" value="27" />
    <field id="selected" type="boolean" value="false" />
    <field id="escape" type="string" value="" />
    <field id="enableBorder" type="boolean" value="true" />
    <field id="borderSize" type="integer" value="6" />
    <field id="enabled" type="boolean" value="true" onChange="onEnabledChanged" />
    <field id="width" type="integer" value="0" />
    <field id="minWidth" type="integer" value="60" />
    <field id="minHeight" type="integer" value="0" />
    <field id="buttonSelected" type="boolean" value="false" alwaysNotify="true" />
  </interface>
</component>