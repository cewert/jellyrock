<?xml version="1.0" encoding="utf-8"?>
<component name="UserSelect" extends="JRScreen">
  <children>
    <LabelPrimaryLarge text="Select a User" horizAlign="center" height="100" width="1920" translation="[0, 200]" />

    <UserRow id="userRow" translation="[130, 360]" />

    <TextButton id="manualLoginButton" minWidth="300" text="Manual Login" translation="[0,850]" />
  </children>
  <interface>
    <field id="itemSelected" type="string" alwaysNotify="true" />
    <field id="itemContent" type="array" onChange="itemContentChanged" />
  </interface>
</component>