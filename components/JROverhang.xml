<?xml version="1.0" encoding="utf-8"?>
<component name="JROverhang" extends="Group">
  <children>
    <!-- Logo -->
    <Poster id="logo" translation="[96, 54]" uri="pkg:/images/branding/logo.png" height="39" width="180" />
    <!-- Title -->
    <ScrollingLabelPrimaryLarge translation="[450, 51]" id="title" vertAlign="center" maxWidth="300" repeatCount="0" />
    <!-- User -->
    <LayoutGroup id="userGroup" layoutDirection="horz" itemSpacings="[15]" translation="[1450, 54]">
      <LabelSecondaryMedium id="username" width="219" horizAlign="left" vertAlign="center" />
    </LayoutGroup>
    <!-- Clock -->
    <Clock />

  </children>
  <interface>
    <field id="id" type="string" />
    <field id="currentUser" type="string" onChange="currentUserChanged" />
    <field id="title" type="string" onChange="titleChanged" />
    <field id="isVisible" value="true" type="boolean" onChange="isVisibleChanged" />
    <field id="disableMoveAnimation" value="false" type="boolean" />
    <field id="isLogoVisible" value="true" type="boolean" onChange="isLogoVisibleChanged" />
  </interface>
</component>