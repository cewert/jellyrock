{
    "$schema": "https://docs.renovatebot.com/renovate-schema.json",
    "extends": [
        "config:base",
        ":disableDependencyDashboard",
        ":enableVulnerabilityAlertsWithLabel(security)"
    ],
    "addLabels": [
        "dependencies"
    ],
    "rebaseWhen": "conflicted",
    "ignorePaths": [
        "**/node_modules/**"
    ],
    "timezone": "America/New_York",
    "packageRules": []
}